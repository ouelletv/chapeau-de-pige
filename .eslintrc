{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": false
        }
    },

  "env": {
      "browser": true,
      "es6": true
    },
    "rules": {
        "semi": 0,
        "no-console": 1
    }
}
